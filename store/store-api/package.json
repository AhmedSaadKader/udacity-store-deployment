{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "nodemon src/server.ts",
    "tsc": "npx tsc",
    "createdb": "db-migrate db:create item_store",
    "migrate": "db-migrate up",
    "watch": "tsc-watch --onSuccess \"node ./dist/server.js\"",
    "jasmine": "jasmine",
    "test": "set ENV=test&& db-migrate db:drop item_store_test && db-migrate db:create item_store_test && db-migrate --env test up && npm run tsc && npm run jasmine && db-migrate db:drop item_store_test",
    "lint": "eslint src/**/*.ts",
    "prettier": "prettier --config .prettierrc --write src/**/*.ts",
    "clean": "rm -rf dist/ || true",
    "build": "npm install . && npm run clean && npm run tsc && cp -r .elasticbeanstalk dist/.elasticbeanstalk && cp package.json dist/package.json && cd dist && zip -r Archive.zip . && cd ..",
    "deploy": "npm run build && eb list && eb use Storefrontbackend-env && eb deploy"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1318.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.8.0",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.15",
    "@types/jasmine": "^4.3.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.6.6",
    "@types/prettier": "^2.7.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0"
  }
}
